crossorigin="anonymous"
></script>
<script type="text/javascript" src="/javascripts/custom/pagenation.js"></script>
<script src="http://code.jquery.com/jquery-1.8.3.js"></script>
<script src="http://code.jquery.com/ui/1.10.0/jquery-ui.js"></script>
<script type="text/javascript" src="/javascripts/jquery.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bubbly-bg@1.0.0/dist/bubbly-bg.js"></script>
<script>
    bubbly({
        blur: 5,
        bubbleFunc: () => `hsla(${Math.random() * 350}, 90%, 50%, ${Math.random() * 0.1})`,
        canvas: document.querySelector("#back"),
        colorStart: '#fff',
        colorStop: '#fff',
        compose: 'source-over',
        shadowColor: '#ffffef',
        velocityFunc: () => Math.random() * 1.4,
        radiusFunc: () => 3 + Math.random() * 20,
    });
</script>
</body>
</html>