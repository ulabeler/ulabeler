                    <button id="stp36"><img src="/images/system/stamp/stamp36.png" alt=""></button>
                </div>
                <div class="pagenation">
                    <!-- ここの書き方分からん -->
                    <button id="page1">1</button>
                    <button id="page2">2</button>
                    <button id="page3">3</button>
                </div>
            </div>
            <div class="div3">
                <button id="" class="customize_button_syousai">
                    画像選択
                </button>
                <button id="gridbtn" class="customize_button_syousai">
                    グリッド
                </button>
                <button id="save" class="customize_button_syousai">
                    保存                
                </button>
                <!-- <form method="post" id="3D" onsubmit="btnsend1()">
                    <input type="hidden" name="finish" value="3D">
                </form> -->
                <button>
                    3DVIEW
                </button>
                <div class="">
                    <p id=""><%= object_name %></p>
                </div>
            </div>
            <div class="div4" id="view">
                <div id="container">
                </div>
            </div>
            <div class="div5">
                <div class="customise_double_button">
                    <button type="button" class="white_button">
                        <div id="js-show-popup">素体選択</div>
                    </button><!-- 購入確認画面へ -->
                    <form method="post" onsubmit="btnsend()" id="finish">
                        <input type="hidden" name="finish" value="finish">
                        <button>終了</button>
                    </form>
                </div>
                <div class="popup" id="js-popup">
                    <div class="popup-inner">
                        <div class="close-btn" id="js-close-btn"><i class="fas fa-times"></i></div>
                        <section class="box_item_3">
                            <br>
                            <h3>
                                <font color="red">素体選択</font>へ<font color="red">戻り</font>ますか？
                            </h3>
                            <br>
                            <p>カスタマイズされた内容は保存されません。</p>
                            <br>
                            <div class="double_button4">
                                <button type="button" class="white_button2"
                                        onclick="location.href='/customize/select_object'">はい</button>
                                <button type="submit" class="red_button2" form=="hoge"
                                        onclick="location.href='/customize/customize_editing'">いいえ</button>
                            </div>
                        </section>
                        <!-- 作品詳細（自分）ここまで👆 -->
                    </div>
                    <div class="black-background" id="js-black-bg"></div>
                </div>
                <script src="/javascripts/custom/pop-up.js"></script>
            </div>
        </div>
    </div>
    </div>
<script type="text/javascript" src="/javascripts/custom/konva.min.js"></script>
