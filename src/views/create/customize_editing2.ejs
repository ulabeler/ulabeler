<body>
    <header>
        <h5 class="h5">素体選択 ＞ カラー選択 ＞<font size="5"> カスタマイズ</font> ＞ 作品設定 ＞ 作品設定確認</h5>
    </header>
    <div class="grid-area">
        <div class="parent">
            <div class="div1">
                <div>
                    <p>テキスト</p>
                    <p id="selecterp"></p>
                    <form method="post" action="" onsubmit="return form1submit()">
                        <select name="font_style" id="font_style"><br>
                            <option id="Arial" value="Arial">Arial</option>
                            <option id="Shizuru" value="Shizuru">Shizuru</option>
                            <option id="Indie Flower" value="Indie Flower">Indie Flower</option>
                            <option id="source sans pro" value="source sans pro">source sans pro</option>
                            <option id="slabo 27px" value="slabo 27px">slabo 27px</option>
                            <option id="slabo 13px" value="slabo 13px">slabo 13px</option>
                            <option id="Raleway" value="Raleway">Raleway</option>
                            <option id="PT sans" value="PT sans">PT sans</option>
                            <option id="merriweather" value="merriweather">merriweather</option>
                            <option id="EB Garamond" value="EB Garamond">EB Garamond</option>
                            <option id="Noto sons" value="Noto sons">Noto sons</option>
                            <option id="Century" value="Century">Century</option>
                            <option id="源ノ角ゴシック" value="源ノ角ゴシック">源ノ角ゴシック</option>
                            <option id="メイリオ" value="メイリオ">メイリオ</option>
                            <option id="ヒラギノ" value="ヒラギノ">ヒラギノ</option>
                            <option id="游明朝" value="游明朝">游明朝</option>
                            <option id="游ゴシック" value="游ゴシック">游ゴシック</option>
                            <option id="Roboto" value="Roboto">Roboto</option>
                            <option id="Lato" value="Lato">Lato</option>
                            <option id="Montserrat" value="Montserrat">Montserrat</option>
                            <option id="Oswald" value="Oswald">Oswald</option>
                        </select>
                        <input type="number" id="number" value="16">
                        <input type="color" id="color"><br>
                    </form>
                    <form onsubmit="return form2submit()">
                        <!-- <textarea id="message" name="message" cols="25" rows="10"></textarea> -->
                        <input type="text" name="message" id="message">
                        <input type="submit">
                    </form>
                    <button id="deletebtn">消す</button>
                </div>
            </div>
            <div class="div2">
                <p>スタンプ</p>
                <div class="stp stp1 current" >
                    <button id="stp10"><img src="/images/system/stamp/stamp10.png" alt=""></button>
                    <button id="stp11"><img src="/images/system/stamp/stamp11.png" alt=""></button>
                    <button id="stp12"><img src="/images/system/stamp/stamp12.png" alt=""></button>
                    <button id="stp13"><img src="/images/system/stamp/stamp13.png" alt=""></button>
                    <button id="stp14"><img src="/images/system/stamp/stamp14.png" alt=""></button>
                    <button id="stp15"><img src="/images/system/stamp/stamp15.png" alt=""></button>
                    <button id="stp16"><img src="/images/system/stamp/stamp16.png" alt=""></button>
                    <button id="stp17"><img src="/images/system/stamp/stamp17.png" alt=""></button>
                    <button id="stp18"><img src="/images/system/stamp/stamp18.png" alt=""></button>
