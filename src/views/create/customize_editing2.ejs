                    <button id="stp28"><img src="/images/system/stamp/stamp27.png" alt=""></button>
                </div>
                <div class="stp stp3" >
                    <button id="stp28"><img src="/images/system/stamp/stamp28.png" alt=""></button>
                    <button id="stp29"><img src="/images/system/stamp/stamp29.png" alt=""></button>
                    <button id="stp30"><img src="/images/system/stamp/stamp30.png" alt=""></button>
                    <button id="stp31"><img src="/images/system/stamp/stamp31.png" alt=""></button>
                    <button id="stp32"><img src="/images/system/stamp/stamp32.png" alt=""></button>
