{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.eslint.json",
    "tsconfigRootDir": ["./src", "./src/TypeAlias", "./src/routes"]
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "max-len": "off",
    "no-unused-vars": "off"
  }
}
